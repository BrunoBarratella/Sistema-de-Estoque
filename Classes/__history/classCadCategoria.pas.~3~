unit classCadCategoria;

interface

uses  System.Classes,
      Vcl.Controls,
      Vcl.ExtCtrls,
      Vcl.Dialogs;

type
  TClassCadCategoria = class
  private

  public
    constructor Create;
    destructor Destroy; override;

  published

  end;


implementation


{ TClassCadCategoria }

